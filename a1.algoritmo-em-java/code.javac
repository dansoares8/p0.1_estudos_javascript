public void algoritmo (int[] v){
    int m;
    int tmp;
    for(int = 0; i < v . length; i++){
        m = i;

        for (int j=i+1; j < v . length; j++){
            if(v[j] < v[m]){
                m=j;
            }
        }
        if(m!=i){
            tmp = v[m];
            v[m] = v[i];
            v[i] = tmp;
        }
    }
}